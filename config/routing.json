{
    "/": "HomeController:homePage",
    "/profile": "ProfileClientController:profileClient",
    "/foodtruck/(:)": "FoodTruckProfileController:vueProfile",
    "/search": "SearchController:searchPage",
    "/testcalendar": "DefaultController:getCalendar",
    "/inscription": "InscriptionController:control",
    "/inscriptionTrucks": "InscriptionTrucksController:control",
    "/login": "DefaultController:login",
    "/administration": "AdminController:adminPage",
    "/testtc": "UtilisateurController:testMe",
    "/logout": "DefaultController:logout",
    "/token": "DefaultController:token",
    "/api/default":{
        "GET":"DefaultController:getDatasFromGET",
        "POST":"DefaultController:getDatasFromPOST",
        "PUT":"DefaultController:getDatasFromPUT",
        "DELETE":"DefaultController:delete"
    },
    "/api/utilisateur/(:)":{
        "GET":"UtilisateurController:retrieve",
        "POST":"UtilisateurController:update",
        "DELETE":"UtilisateurController:delete"
    },
    "/api/trucks/(:)/events":{
        "GET":"TruckEvenementController:getAllEventsForCalendar"
    },
    "/api/trucks/(:)/planning":{
        "GET":"PlanningController:getAllPlanningForCalendar",
        "POST":"PlanningController:updateThePlanning"
    },
    "/api/planning/update":{
        "POST":"PlanningController:updateThePlanning"
    },
    "/api/Favoris/delete/utilisateur/(:)/foodtruck/(:)":{
        "POST":"FavorisController:delete"
    },
    "/api/Favoris/add/utilisateur/(:)/foodtruck/(:)":{
        "POST":"FavorisController:create"
    },
    "/api/Avis/add/utilisateur/(:)/commande/(:)/foodtruck/(:)":{
        "POST":"AvisController:create"
    }
}
